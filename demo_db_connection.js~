
let mysql = require("mysql");

let con = mysql.createConnection({
    host: "localhost",
    user: "brendan",
    password: "password"
});

con.connect(function(err) {
    if (err) throw err;
    console.log("connected!");
    let sql = "SHOW DATABASES;"
    con.query(sql, function (err, result) {
	if (err) throw err;
	for (obj in result) {
	    for (key in obj) {
		console.log("Result: " + obj[key] + " " + result[obj[key]]);
	    }
	}
    });
});
    
